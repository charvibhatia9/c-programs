#include <stdio.h>
#include <math.h>

int main()
{
    int f,p, r, t ,c,b;

    printf("enter the principle interest:");
    scanf("%d" , &p);
    
    printf("enter the rate of interest:");
    scanf("%d" , &r);
    
    printf("enter the number of years:");
    scanf("%d" , &t);
//f=p+[(1+(r/100))]^t
     c=1+(r/100);
     b= c^t;
     f=p+b;
    printf("The compound interest : %d", f);

    return 0;
}
